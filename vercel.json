{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "https://trip-finder-app.onrender.com",
    "VITE_WS_URL": "wss://trip-finder-app.onrender.com"
  }
}
